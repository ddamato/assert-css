{
  "name": "assert-css",
  "version": "1.0.1",
  "description": "Assert that CSS is constructed properly",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "unbuild": {
    "entries": [
      "src/index"
    ],
    "clean": true,
    "rollup": {
      "emitCJS": true
    }
  },
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "npm test && npm run build",
    "test": "mocha",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "keywords": [
    "assert",
    "assertion",
    "css",
    "styles",
    "test",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddamato/assert-css.git"
  },
  "bugs": {
    "url": "https://github.com/ddamato/assert-css/issues"
  },
  "homepage": "https://github.com/ddamato/assert-css",
  "author": "Donnie D'Amato <fauxserious@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "mocha": "^10.4.0",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "css-tree": "^2.3.1"
  }
}
